<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <validationRules>
        <fullName>CEC_RestrictARCaseOwnerShip</fullName>
        <active>true</active>
        <description>When assigning the AR Case to the queue, only the most recent queue will be allowed to be selected</description>
        <errorConditionFormula>AND(
NOT($Setup.UPSD_Bypass_Validation_Rules_Triggers__c.Bypass_Case_Validation_rules__c),
RecordType.DeveloperName = &apos;CEC_Assistance_Required_Case&apos;,
CEC_Updated_from_IWT__c = false,
NOT($Profile.Name = &apos;UPS CEC Admin&apos;),
NOT($Profile.Name == &apos;System Administrator&apos;),
 AND (ISCHANGED(OwnerId), ISCHANGED(CEC_Original_Queue_Name__c)) 
)</errorConditionFormula>
        <errorMessage>Cases cannot be assigned to this queue or user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CEC_RestrictContactCaseOwnership</fullName>
        <active>true</active>
        <description>PBI: 3566677.  This validation restrict Contact Cases to be assigned to SRC queues.</description>
        <errorConditionFormula>AND(
	NOT($Setup.UPSD_Bypass_Validation_Rules_Triggers__c.Bypass_Case_Validation_rules__c),
	RecordType.DeveloperName = &apos;CEC_Case&apos;,
	ISCHANGED(OwnerId),
	CEC_Updated_from_IWT__c = false,
        NOT($Profile.Name == &apos;System Administrator&apos;),
	NOT($Profile.Name = &apos;UPS CEC Admin&apos;), 
	CONTAINS(CEC_Original_Queue_Name__c, &apos;CEC&apos;),
	CONTAINS(CEC_Original_Queue_Name__c, &apos;SRC&apos;)
)</errorConditionFormula>
        <errorMessage>Cases cannot be assigned to this queue or user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CEC_Restrict_SSR_And_Specialized_Users</fullName>
        <active>false</active>
        <errorConditionFormula>IF((CEC_Updated_from_IWT__c =false),
IF( 
  AND(
    OR( $Profile.Name == &quot;UPS CEC CCR&quot;,  $Profile.Name == &quot;UPS CEC Specialized User&quot;),
    ISCHANGED(OwnerId),
    Case_Owner_Role__c == null,    
    CONTAINS(CEC_Original_Queue_Name__c, &quot;CEC&quot;),
    CONTAINS(CEC_Original_Queue_Name__c, &quot;SRC&quot;) 
  ),
  True, 
  False
),
  False
)</errorConditionFormula>
        <errorMessage>Cases cannot be assigned to this queue or user.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CEC_Restrict_UPS_CEC_SRC</fullName>
        <active>false</active>
        <errorConditionFormula>IF((CEC_Updated_from_IWT__c =false),
IF(
AND(
$Profile.Name==&quot;UPS CEC SRC User&quot;,
ISCHANGED(OwnerId),
OR (
Case_Owner_Role__c != null,
AND (
CONTAINS(CEC_Original_Queue_Name__c, &quot;CEC&quot;),
NOT(CONTAINS(CEC_Original_Queue_Name__c, &quot;SRC&quot;))))
),
True,
False
),
False
)</errorConditionFormula>
        <errorMessage>Cases cannot be assigned to this queue or user.</errorMessage>
    </validationRules>
</CustomObject>
